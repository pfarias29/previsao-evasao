{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Streamlit App",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "app.py",
        "--server.port=8501",
        "--server.headless=true",
      ],
      "justMyCode": false,
      "console": "integratedTerminal",
      "env": {
        "STREAMLIT_ENV": "development"
      }
    }
  ]
}
